package quiz22;

import java.util.regex.Matcher;
import java.util.regex.Pattern;

public class ReGextQuiz02 {
	
	public static void main(String[] args) {
		
		String str = "123123-1231231 GS25(치킨마요) 4,400원";
		String str2 = "123123-1231232 GS25(주부18단) 5,000원";
		
		String pattern1 = "[0-9-]{6,7}";
		String pattern2 = "[A-Z]+[0-9]+";
		String pattern3 = "[(가-힣(18)]{2,10}";
		String pattern4 = "[0-9,]+원";
		
		Matcher m = Pattern.compile(pattern1).matcher(str);
		Matcher m1 = Pattern.compile(pattern2).matcher(str);
		Matcher m2 = Pattern.compile(pattern3).matcher(str);
		Matcher m3 = Pattern.compile(pattern4).matcher(str);
		
		while(m.find()) {
			
			System.out.print(m.group());
		
			
		}
		
	}

}
